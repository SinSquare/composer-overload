{
    "name": "sinsquare/composer-overload",
    "type": "library",
    "description": "A simple tool to overload classes using composer",
    "license": "MIT",
    "authors": [
        {
            "name": "Abel Katona"
        }
    ],
    "require": {
        "php": "*",
        "composer/composer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "ComposerOverload\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "ComposerOverload\\Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}